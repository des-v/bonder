{
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "plugins": [
        "jest/recommended"
    ],
    "rules": {
        "prettier/prettier": "error",
        "eqeqeq": "off",
        "class-methods-use-this": "off",
        "no-underscore-dangle": "off"
    },
    "env": {
        "jest/globals": true
    }
}