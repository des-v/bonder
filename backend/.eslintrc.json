{
    "extends": [
        "airbnb",
        "prettier"
    ],
    "plugins": [
        "jest"
    ],
    "rules": {
        "eqeqeq": "off",
        "class-methods-use-this": "off",
        "no-underscore-dangle": "off"
    },
    "env": {
        "jest/globals": true
    },
    "settings": {
        "react": {
            "version": "latest"
        }
    }
}